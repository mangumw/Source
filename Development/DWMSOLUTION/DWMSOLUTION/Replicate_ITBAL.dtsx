                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select 	t1.IBITNO,
	t1.IBWHID,
	case
	  when t1.IBOHQ1 - t1.IBAQT1 &lt; 0
                  then 0
