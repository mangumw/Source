              <ProjectItem
                Name="ScriptMain.vb"
                Encoding="UTF8"><![CDATA[' Microsoft SQL Server Integration Services Script Task
' Write scripts using Microsoft Visual Basic
' The ScriptMain class is the entry point of the Script Task.

Imports System
Imports System.Data
Imports System.Math
Imports Microsoft.SqlServer.Dts.Runtime

<Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute>
<System.CLSCompliantAttribute(False)>
Partial Public Class ScriptMain
    Inherits Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase

    Enum ScriptResults
        Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success
        Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
    End Enum

    ' The execution engine calls this method when the task executes.
    ' To access the object model, use the Dts object. Connections, variables, events,
    ' and logging features are available as static members of the Dts class.
    ' Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
    ' 
    ' To open Code and Text Editor Help, press F1.
    ' To open Object Browser, press Ctrl+Alt+J.

    Public Sub Main()
        '
        Dim Email As String
        Dim EMailSql As String
        Dim Pub_Name As String
        Dim Where_Clause As String
        Dim strsql As String
        Dim Log_Sql As String
        Dim Zip As String
        Dim XLS_Filename As String
        Dim ZIP_Filename As String

        Pub_Name = CType(Dts.Variables("Pub_Name").Value, String)
        Where_Clause = CType(Dts.Variables("Where_Clause").Value, String)
        Email = CType(Dts.Variables("EMail").Value, String)
        Zip = CType(Dts.Variables("Zip").Value, String)
        XLS_Filename = "E:\Extracts\" + Pub_Name + "_Export.xls"
        ZIP_Filename = "E:\Extracts\" + Pub_Name + "_Export.zip"
        Dts.Variables("XLS_Filename").Value = XLS_Filename
        Dts.Variables("ZIP_Filename").Value = ZIP_Filename

        If Zip = "0" Then
            EMailSql = "EXEC msdb.dbo.sp_send_dbmail @profile_name = 'infausr', @recipients = '" + Email + "'"
            EMailSql = EMailSql + ",@body = 'Attached you will find an updated listing of sales by item. "
            EMailSql = EMailSql + "If you have any questions concerning this data, please contact BAM Business Intelligence at big@booksamillion.com',"
            EMailSql = EMailSql + "@subject = " + "'* Updated * Books-A-Million Sales Data Extract For " + Pub_Name + "',"
            EMailSql = EMailSql + "@file_attachments = '" + XLS_Filename + "'"
            Dts.Variables("Attach").Value = XLS_Filename


        Else
            EMailSql = "EXEC msdb.dbo.sp_send_dbmail @profile_name = 'infausr', @recipients = '" + Email + "'"
            EMailSql = EMailSql + ",@body = 'Attached you will find a current listing of sales by item. "
            EMailSql = EMailSql + "If you have any questions concerning this data, please contact BAM Business Intelligence at big@booksamillion.com',"
            EMailSql = EMailSql + "@subject = " + "'Books-A-Million Sales Data Extract For " + Pub_Name + "',"
            EMailSql = EMailSql + "@file_attachments = '" + ZIP_Filename + "'"
            Dts.Variables("Attach").Value = ZIP_Filename

        End If


        Dts.Variables("EMailSql").Value = EMailSql

        Log_Sql = "Insert into reference.dbo.Vendor_Export_Log Values(Getdate(),'"
        Log_Sql = Log_Sql + Pub_Name + "','"
        Log_Sql = Log_Sql + Email + "')"

        Dts.Variables("Log_Sql").Value = Log_Sql

        strsql = "Select cast('" + Pub_Name + "' as NVARCHAR(255)) as Pub_Group, cast(ISBN as NVARCHAR(255)) as ISBN,cast(Sku_Number as NVARCHAR(255)) as Sku_Number, cast(Replace(Title,',',' ') as nvarchar(255)) AS Title, cast(replace(Author,',',' ') as nvarchar(255)) as Author, cast(Sku_Type as nvarchar(255)) as Sku_Type, IDate,"
        strsql = strsql + "Retail,Week1Units as Week1Units,Week2Units as Week2Units,"
        strsql = strsql + "TYYTDUnits as YTD_Units,BAM_OnHand,Warehouse_OnHand,Qty_OnOrder"
