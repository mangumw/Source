        <SQLTask:SqlTaskData
          SQLTask:Connection="{124CCD95-5F87-4C6D-BE32-02F919B9D0BC}"
          SQLTask:SqlStatementSource="declare @Start_Date int&#xA;declare @End_Date int&#xA;declare @Ls smalldatetime&#xA;select @ls = staging.dbo.fn_Last_Saturday(getdate())&#xA;select @ls = dateadd(dd,3,@ls)&#xA;select @Start_Date = staging.dbo.fn_DateToInt(@ls)&#xA;select @ls = dateadd(dd,6,@ls)&#xA;select @End_Date = staging.dbo.fn_DateToInt(@ls)&#xA;&#xA;truncate table reportdata.dbo.discounts&#xA;insert into reportdata.dbo.discounts&#xA;SELECT distinct t2.ISBN&#xA;      ,t1.PLNITM&#xA;      ,t1.PLNTYP&#xA;      ,t1.PLNCDT&#xA;      ,t1.PLNADT&#xA;      ,t1.PLNAMT&#xA;      ,t1.PLNFLG&#xA;      ,t2.POS_Price&#xA;      ,(t2.POS_Price-t1.PLNAMT)/t2.POS_Price AS Discount&#xA;  FROM Reference.dbo.prcpln t1,&#xA;&#x9;   reference.dbo.item_master t2&#xA;where&#x9;t2.sku_number = t1.plnitm&#xA;and&#x9;&#x9;plncdt = @Start_Date&#xA;and&#x9;&#x9;plnadt = @End_Date&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
