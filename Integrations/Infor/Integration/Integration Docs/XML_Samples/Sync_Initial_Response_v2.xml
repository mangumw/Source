<?xml version="1.0" encoding="UTF-8"?>
<BackgroundReports>
	<BackgroundReportPackage type="status" updateType="Synchronous">
		<!-- This is an internally used reference to the new order that consists of the client account number and a newly 
			generated order ID; This value has no use/bearing to the client and would only have value for debug/diagnostic purposes -->
		<ProviderReferenceId>
			<IdValue>ED-xxxxxxx_3939</IdValue>
		</ProviderReferenceId>
		<!-- This will be an echo back to Infor of the value we extracted from the node in the original order of:
			/BackgroundCheck/BackgroundSearchPackage/ReferenceId[@idOwner='Infor']/IdValue[@name='CandidateId']
		-->
		<ClientReferenceId>
			<IdValue>$numericIdentifer$</IdValue>
		</ClientReferenceId>
		<ScreeningStatus>
			<OrderStatus>Acknowledged</OrderStatus>
			<DateOrderReceived>09/15/2021</DateOrderReceived>
		</ScreeningStatus>
		<SupportingDocumentation>
			<Documentation>
				<!-- This URL will allow user to redirect back to Edge system either to confirm the order being placed, view the order status, or the order detail with result -->
				<InternetWebAddress><![CDATA[https://$edgeSystemURL$/index.cfm?key=$numericIdentifer$]]></InternetWebAddress>
			</Documentation>
		</SupportingDocumentation>
	</BackgroundReportPackage>
</BackgroundReports>